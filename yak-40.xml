                </product>
            </function>
            <function name="aero/coefficient/CLground">
                <description>ground lift reaction</description>
                <product>
                    <table name="Delta lift due ground reaction">
                        <independentVar>position/h-agl-ft</independentVar>
                        <tableData>
                          0             0.35        
                          5             0.3         
