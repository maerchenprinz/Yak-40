<!--AP Pitch Up-->
   <key n="357">
   <name>Arrow Up</name>
   <desc>AP Pitch DOWN</desc>
    <binding>
     <command>nasal</command>
     <script>
	if ( getprop("yak-40/AP-40/ap_on") == 0 ) {
	controls.incElevator(0.05, 1.0);
	}
	else if ( getprop("yak-40/AP-40/pitch_channel") == 1 ) {
		setprop("autopilot/locks/altitude", "pitch-hold");
		setprop("autopilot/internal/target-pitch-deg", (getprop("autopilot/internal/target-pitch-deg") - 0.5));
		}
     </script>
    </binding>
   </key>
            
<!--AP Down-->
   <key n="359">
   <name>Arrow Down</name>
   <desc>AP Pitch UP</desc>
    <binding>
     <command>nasal</command>
     <script>
	if ( getprop("yak-40/AP-40/ap_on") == 0 ) {
	controls.incElevator(-0.05, -1.0);
	}
	else if ( getprop("yak-40/AP-40/pitch_channel") == 1 ) {
		setprop("autopilot/locks/altitude", "pitch-hold");
		setprop("autopilot/internal/target-pitch-deg", (getprop("autopilot/internal/target-pitch-deg") + 0.5));
		}
     </script>
    </binding>
   </key>        
