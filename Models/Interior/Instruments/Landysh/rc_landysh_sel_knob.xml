<?xml version="1.0"?>

<!-- SPU-like thingy right console-->
<PropertyList>
 <path>../../shared/knobs/pm.ac</path>

 <animation>
  <type>knob</type>
  <object-name>pm</object-name>
  <property>yak-40/Landysh/source_viewnr101</property>
  <factor>30.0</factor>
  <offset-deg>-40.0</offset-deg>
   <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
   </center>
   <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
   </axis>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>yak-40/Landysh/source_viewnr101</property>
      <factor>1</factor>
      <min>0</min>
      <max>4</max>
      <wrap>false</wrap>
     </binding>
    </action>
 </animation>

</PropertyList>
