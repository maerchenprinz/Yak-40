<?xml version="1.0"?>

<!--"SPU"-panel left console-->
<PropertyList>
 <path>landysh_sel_panel.ac</path>

 <model>
  <name>spuswitchr</name>
  <path>rc_landysh_sel_knob.xml</path>
  <offsets>
   <x-m>-0.017</x-m>
   <y-m>0.0</y-m>
   <z-m>0.014</z-m>
   <pitch-deg>-90</pitch-deg>
   <heading-deg>0</heading-deg>
  </offsets>
 </model>

<!-- Text OSG -->
  <animation>
    <type>select</type>
    <object-name>textspupanelr</object-name>
    <condition>
      <property>sim/panel-hotspots</property>
    </condition>
  </animation>

  <text>
    <name>textspupanelr</name>
    <type type="string">literal</type>
    <text type="string">Landysh No.1 UHF</text>
    <draw-text type="bool">true</draw-text>
    <font>Helvetica.txf</font>
    <character-size type="double">0.024</character-size>
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <axis-alignment>yz-plane</axis-alignment>
    <max-height>0.05</max-height>
    <max-width>0.08</max-width>
    <font-resolution>
      <width type="int">64</width>
      <height type="int">64</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
     <x-m>0.015</x-m>
     <y-m>0.000</y-m>
     <z-m>0.000</z-m>
    </offsets>
  </text>
  <animation>
    <type>material</type>
    <object-name>textspupanelr</object-name>
    <emission>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
    </emission>
  </animation>

</PropertyList>
