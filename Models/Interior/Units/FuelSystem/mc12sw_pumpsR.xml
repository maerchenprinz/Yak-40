<?xml version="1.0"?>

<!-- AZS-20, Fuel Pumps right -->
<PropertyList>
 <path>../../shared/switches/azs.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/FuelSystem/sw_pump463R", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>pumpron</tooltip-id>
      <label>Right Fuel
Pumps ON</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/FuelSystem/sw_pump463R", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>pumproff</tooltip-id>
      <label>Right Fuel
Pumps OFF</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>yak-40/FuelSystem/sw_pump463R</property>
  <interpolation>
    <entry><ind>1.0</ind><dep>30.0</dep></entry>
    <entry><ind>0.0</ind><dep>-30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
